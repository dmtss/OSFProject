<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <div id="container">
  <header> <%-include('./navBar.ejs')%></header>
  <body>  <h1>title</h1>
    <div class="content">
   
        
    <h1><a href="/categories/<%=varlink%>"><%=category %></a> / <a href="/categories/<%=varlink%>/<%=parentcategory.toLowerCase()%>"><%=parentcategory %></a></h1>

    <ul>

    <% items.forEach(function(parentC) { %>
      
      <% let pname = parentC.name.replace(' ', '-'); %>
      <% pname = pname.replace('&','') %>
      <% pname = pname.replace(' ','') %>

      <li>
          <h1><a href="/categories/<%=varlink%>/<%=parentcategory.toLowerCase()%>/<%=pname.toLowerCase()%>"><%= parentC.name %></a></h1>
          <p><%= parentC.page_description %></p>
          <% if (parentC.image) { %>
          <div>
              <img src="/images/<%= parentC.image %>">
          </div>
          <% } %>
          <div>
              
          </div>
      </li>
      <% }); %>
  </ul>
    </div>

    <footer id="footer">
      OSF Digital, August 2022
    </footer>
    </body>

  </div>
</html>
